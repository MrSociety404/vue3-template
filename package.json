{
  "name": "finances-brussels",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "postinstall": "patch-package && husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint"
  },
  "dependencies": {
    "@prettier/plugin-pug": "^2.2.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "autoprefixer": "^10.4.8",
    "commitlint": "^17.1.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "sass": "^1.54.5",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-svg-loader": "^3.4.0",
    "vue-i18n": "9",
    "vue-tsc": "^0.39.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        2,
        "never",
        []
      ]
    }
  }
}
